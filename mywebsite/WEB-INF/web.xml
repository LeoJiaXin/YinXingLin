<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">

  <display-name>Welcome to Tomcat</display-name>
  <description>
     Welcome to Tomcat
  </description>
  <servlet>
      <!-- servlet-name任意取 -->
      <servlet-name>servlet_one</servlet-name>
      <servlet-class>com.test.my.Servlet_One</servlet-class>
  </servlet>
  <servlet-mapping>
      <!-- 所映射的servlet的名字 -->
      <servlet-name>servlet_one</servlet-name>
      <!-- 浏览器访问该servlet的URL,任意取 -->
      <url-pattern>/liujiaxin</url-pattern>
  </servlet-mapping> 
  <servlet>
      <servlet-name>servlet_two</servlet-name>
      <servlet-class>com.test.my.Servlet_Two</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_two</servlet-name>
      <url-pattern>/test2</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>servlet_three</servlet-name>
      <servlet-class>com.test.my.Servlet_Three</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_three</servlet-name>
      <url-pattern>/love</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>servlet_login</servlet-name>
      <servlet-class>com.test.my.Login</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_login</servlet-name>
      <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>servlet_test</servlet-name>
      <servlet-class>com.test.my.Servlet_Test</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_test</servlet-name>
      <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>servlet_confirm</servlet-name>
      <servlet-class>com.test.my.LoginConfirm</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_confirm</servlet-name>
      <url-pattern>/confirm</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>servlet_img</servlet-name>
      <servlet-class>com.test.my.DeliverImage</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_img</servlet-name>
      <url-pattern>/img</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>servlet_register</servlet-name>
      <servlet-class>com.test.my.Register</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_register</servlet-name>
      <url-pattern>/update</url-pattern>
  </servlet-mapping>
<servlet>
      <servlet-name>servlet_getDish</servlet-name>
      <servlet-class>com.test.my.Loading_Menu</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servlet_getDish</servlet-name>
      <url-pattern>/loadmenu</url-pattern>
  </servlet-mapping>
<servlet>
      <servlet-name>servletGetcanteen</servlet-name>
      <servlet-class>com.skye.servlet.GetCanteenName</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servletGetcanteen</servlet-name>
      <url-pattern>/loadcanteens</url-pattern>
  </servlet-mapping>
<servlet>
      <servlet-name>servletuploaddish</servlet-name>
      <servlet-class>com.test.my.UploadDish</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>servletuploaddish</servlet-name>
      <url-pattern>/upload</url-pattern>
  </servlet-mapping>

</web-app>